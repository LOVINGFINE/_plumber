{
  "compilerOptions": {
      "baseUrl": "./", 
      "esModuleInterop": true, 
      "allowJs": true, 
      "checkJs": true,  
      "jsx": "react", 
      "outDir": "./",
      "noEmit":true,
      "paths": {                 
         "@/components/*": ["./src/components/*"],
         "@/utils/*": ["./src/utils/*"],
         "@/assets/*": ["./src/assets/*"],
         "@/models/*": ["./src/models/*"],
      }
  },
  "exclude": ["node_modules","dist"]
}